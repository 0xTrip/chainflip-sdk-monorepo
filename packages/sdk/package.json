{
  "name": "@chainflip-io/chainflip-sdk",
  "version": "0.0.15",
  "description": "",
  "exports": {
    "./swap": {
      "import": "./dist/mjs/swap/index.js",
      "require": "./dist/cjs/swap/index.js",
      "type": "./dist/mjs/swap/index.d.ts"
    },
    "./swap/types": {
      "import": "./dist/mjs/swap/types/index.js",
      "require": "./dist/cjs/swap/types/index.js",
      "type": "./dist/mjs/swap/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "swap": [
        "dist/cjs/swap/index.d.ts"
      ],
      "swap/types": [
        "dist/cjs/swap/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "test:ci": "CI=1 jest --forceExit",
    "build": "rm -rf dist && tsc --project ./tsconfig.mjs.json && tsc --project ./tsconfig.cjs.json",
    "start": "node ./dist/cjs/swap/index.js",
    "dev": "tsc-watch --noClear --compiler ./node_modules/typescript/bin/tsc --onFirstSuccess \"nodemon\"",
    "typecheck": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "18.15.0",
    "pnpm": ">=7"
  },
  "repository": "https://github.com/chainflip-io/chainflip-sdk.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@chainflip-io/sdk-shared": "workspace:../shared",
    "axios": "^1.3.4"
  }
}
