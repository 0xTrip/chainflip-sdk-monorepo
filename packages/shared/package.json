{
  "name": "@chainflip-io/sdk-shared",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "exports": {
    "./assets": {
      "import": "./dist/mjs/assets.js",
      "require": "./dist/cjs/assets.js",
      "type": "./dist/cjs/assets.d.ts"
    },
    "./parsers": {
      "import": "./dist/mjs/parsers.js",
      "require": "./dist/cjs/parsers.js",
      "type": "./dist/cjs/parsers.d.ts"
    },
    "./schemas": {
      "import": "./dist/mjs/schemas.js",
      "require": "./dist/cjs/schemas.js",
      "type": "./dist/cjs/schemas.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "assets": [
        "dist/mjs/assets.d.ts"
      ],
      "parsers": [
        "dist/mjs/parsers.d.ts"
      ],
      "schemas": [
        "dist/mjs/schemas.d.ts"
      ]
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json",
    "build:abis": "typechain --target=ethers-v5 --out-dir=./src/abis ./abis/**",
    "lint": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "test": "jest",
    "test:ci": "CI=1 pnpm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "zod": "^3.21.4"
  },
  "dependencies": {
    "@polkadot/util": "^12.0.1",
    "@polkadot/util-crypto": "^12.0.1",
    "ethers": "5.7.2"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@typechain/ethers-v5": "^11.0.0",
    "typechain": "^8.2.0",
    "zod": "^3.21.4"
  }
}
