{
  "name": "@chainflip-io/chainflip-sdk",
  "version": "0.0.13",
  "description": "",
  "exports": {
    "./swap": {
      "import": "./dist/mjs/swap/index.js",
      "require": "./dist/cjs/swap/index.js",
      "type": "./dist/mjs/swap/index.d.ts"
    },
    "./swap/types": {
      "import": "./dist/mjs/swap/types/index.js",
      "require": "./dist/cjs/swap/types/index.js",
      "type": "./dist/mjs/swap/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "swap": [
        "dist/cjs/swap/index.d.ts"
      ],
      "swap/types": [
        "dist/cjs/swap/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "build": "rm -rf dist && tsc --project ./tsconfig.json && tsc --project ./tsconfig.cjs.json",
    "start": "node ./dist/cjs/swap/index.js",
    "dev": "tsc-watch --noClear --compiler ./node_modules/typescript/bin/tsc --onFirstSuccess \"nodemon\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "tsc-watch": "^4.6.2",
    "typescript": "^5.0.0",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": "18.15.0",
    "pnpm": ">=7"
  },
  "repository": "https://github.com/chainflip-io/chainflip-sdk.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
